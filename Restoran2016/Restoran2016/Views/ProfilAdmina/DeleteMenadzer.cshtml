@model Restoran2016.Models.MENADZER

@{
    ViewBag.Title = "DeleteMenadzer";
}

<h2>DeleteMenadzer</h2>

<h3>Da li ste sigurni da zelite obrisati ovog menadzera?</h3>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Menadzer:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IDMENADZERA)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Obrisi" class="btn btn-default" /> |
            @Html.ActionLink("Vrati se nazad", null, null, null, new { href = Request.UrlReferrer })
        </div>
    }
</div>
