@model IEnumerable<Restoran2016.ModelView.PozivRestoran>

@{
    ViewBag.Title = "ListaPrijateljaRez";
}

<h2>Lista pozvanih prijatelja</h2>


<table class="table">
    <tr>

        <th>
            @Html.DisplayName("Ime")
        </th>
        <th>
            @Html.DisplayName("Prezime")
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.IME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PREZIME)
        </td>

    </tr>
}

</table>
